{
  "root": true,
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "browser": true,
    "jquery": true,
    "es6": true
  },
  "globals": {
    "Carousel": false,
    "CarouselLite": false
  },
  "rules": {
    "no-cond-assign": ["error", "except-parens"]
  },
  "extends": "airbnb-base"
}
